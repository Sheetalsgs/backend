"family": "backend-ms1-taskdef",
    "containerDefinitions": [
        {
            "name": "ms1",
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "cpu": 0,
            "portMappings": [
                {
                    "name": "ms1-3001-tcp",
                    "containerPort": 3001,
                    "hostPort": 0,
                    "protocol": "tcp",
                    "appProtocol": "http"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [],
            "volumesFrom": [],
            "ulimits": []
        }
    ],
    "executionRoleArn": "arn:aws:iam::651622532734:role/ecsTaskExecutionRole",
    "networkMode": "bridge",
    "requiresCompatibilities": [
        "EC2"
    ],
    "cpu": "512",
    "memory": "512",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
